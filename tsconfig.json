{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "esnext",
    "moduleResolution": "node",
    "diagnostics": false,
    "extendedDiagnostics": false,
    "paths": {
      "@lightbulb/lib/*": ["lib/*"],
      "@lightbulb/src/*": ["src/*"]
    }
  },
  "include": ["src/**/*", "lib/**/*"],
  "compileOnSave": false,
  "exclude": ["dist/**"],
  "extends": "@splatterxl/tsconfig"
}
